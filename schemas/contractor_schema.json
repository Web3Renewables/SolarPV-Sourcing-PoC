{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://web3renewables.com/contractor_role_schema.json",
  "title": "Contractor Role Schema",
  "description": "Schema describing information collected from Contractor's at point of registration",
  "type": "object",
  "properties": {
    "building_contractor_name": {
      "type": "string",
      "description": "Name of the building contractor"
    },
    "building_contractor_address": {
      "type": "string",
      "description": "Address of the building contractor"
    },
    "building_contractor_phone_number": {
      "type": "string",
      "description": "Phone number of the building contractor"
    },
    "building_contractor_class_type": {
      "type": "string",
      "description": "Building contractor class type"
    },
    "building_contractor_license_number": {
      "type": "string",
      "description": "Building contractor license number"
    },
    "building_contractor_application_number": {
      "type": "string",
      "description": "Building contractor application number"
    },
    "building_contractor_status": {
      "type": "string",
      "description": "Building contractor license status",
      "enum": ["issued", "revoked"]
    },
    "building_contractor_enforcement_action": {
      "type": "string",
      "description": "Building contractor enforcement action description"
    },
    "building_contractor_enforcement_action_radio": {
      "type": "string",
      "description": "Building contractor enforcement action status",
      "enum": ["yes", "no"]
    },
    "building_contractor_expiration_date": {
      "type": "string",
      "description": "Building contractor license expiration date - ISO 8601"
    },
    "building_contractor_effective_date": {
      "type": "string",
      "description": "Building contractor license effective date - ISO 8601"
    },
    "building_contractor_origination_date": {
      "type": "string",
      "description": "Building contractor license origination date - ISO 8601"
    },
    "building_contractor_print_date": {
      "type": "string",
      "description": "Building contractor license print date - ISO 8601"
    },
    "qualifying_builder_name": {
      "type": "string",
      "description": "Name of the qualifying builder"
    },
    "qualifying_builder_address": {
      "type": "string",
      "description": "Address of the qualifying builder"
    },
    "qualifying_builder_phone_number": {
      "type": "string",
      "description": "Phone number of the qualifying builder"
    },
    "qualifying_builder_class_type": {
      "type": "string",
      "description": "Qualifying builder class type"
    },
    "qualifying_builder_license_number": {
      "type": "string",
      "description": "Qualifying builder license number"
    },
    "qualifying_builder_application_number": {
      "type": "string",
      "description": "Qualifying builder application number"
    },
    "qualifying_builder_status": {
      "type": "string",
      "description": "Qualifying builder license status",
      "enum": ["issued", "revoked"]
    },
    "qualifying_builder_enforcement_action": {
      "type": "string",
      "description": "Qualifying builder enforcement action description"
    },
    "qualifying_builder_enforcement_action_radio": {
      "type": "string",
      "description": "Qualifying builder enforcement action status",
      "enum": ["yes", "no"]
    },
    "qualifying_builder_expiration_date": {
      "type": "string",
      "description": "Qualifying builder license expiration date - ISO 8601"
    },
    "qualifying_builder_effective_date": {
      "type": "string",
      "description": "Qualifying builder license effective date - ISO 8601"
    },
    "qualifying_builder_origination_date": {
      "type": "string",
      "description": "Qualifying builder license origination date - ISO 8601"
    },
    "qualifying_builder_print_date": {
      "type": "string",
      "description": "Qualifying builder license print date - ISO 8601"
    }
  },
  "required": [
    "building_contractor_name",
    "building_contractor_address",
    "building_contractor_phone_number",
    "building_contractor_class_type",
    "building_contractor_license_number",
    "building_contractor_application_number",
    "building_contractor_status",
    "building_contractor_enforcement_action_radio",
    "building_contractor_expiration_date",
    "building_contractor_effective_date",
    "building_contractor_origination_date",
    "qualifying_builder_name",
    "qualifying_builder_address",
    "qualifying_builder_phone_number",
    "qualifying_builder_class_type",
    "qualifying_builder_license_number",
    "qualifying_builder_application_number",
    "qualifying_builder_status",
    "qualifying_builder_enforcement_action_radio",
    "qualifying_builder_expiration_date",
    "qualifying_builder_effective_date",
    "qualifying_builder_origination_date"
  ]
}