{
  "name": "server",
  "version": "0.1.0",
  "description": "AWS Server Code for Creating GCs",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "runner:ec2:dev": "node -r dotenv/config src/ec2_runner.js dotenv_config_path=.env.local",
    "runner:ec2:prod": "node -r dotenv/config src/ec2_runner.js dotenv_config_path=.env.production",
    "daily:volta": "node -r dotenv/config src/volta/daily_runner.js dotenv_config_path=.env.local",
    "daily:ewc": "node -r dotenv/config src/ewc/daily_runner.js dotenv_config_path=.env.production",
    "monthly:volta": "node -r dotenv/config src/volta/monthly_runner.js dotenv_config_path=.env.local",
    "monthly:ewc": "node -r dotenv/config src/ewc/monthly_runner.js dotenv_config_path=.env.production",
    "monthly-precheck:volta": "node -r dotenv/config src/volta/monthly_precheck_runner.js dotenv_config_path=.env.local",
    "monthly-precheck:ewc": "node -r dotenv/config src/ewc/monthly_precheck_runner.js dotenv_config_path=.env.production",
    "create-initial-index:volta": "node -r dotenv/config src/volta/create-initial-index.js dotenv_config_path=.env.local",
    "create-initial-index:ewc": "node -r dotenv/config src/ewc/create-initial-index.js dotenv_config_path=.env.production",
    "fileUpload:test": "node -r dotenv/config src/uploadExampleFiles.js dotenv_config_path=.env.local",
    "vcIssunace:test": "node -r dotenv/config src/utils/scripts/issueClaim.js dotenv_config_path=.env.local",
    "gcsFromMonthly:test": "node -r dotenv/config src/utils/scripts/getPVSystemGCs.js dotenv_config_path=.env.local",
    "w3updates:test": "node -r dotenv/config src/utils/scripts/testW3NameIndex.js dotenv_config_path=.env.local",
    "batchDailyGCs:test": "node -r dotenv/config src/utils/scripts/create_batch_gcs_range.js dotenv_config_path=.env.local",
    "checkAndUpdatePastGCs:test": "node -r dotenv/config src/utils/scripts/check_and_update_past_gcs.js dotenv_config_path=.env.local",
    "test": "mocha --timeout 10000 src/tests -r dotenv/config dotenv_config_path=.env.local"
  },
