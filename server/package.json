    "@ew-did-registry/jwt": "0.8.1-alpha.996.0",
    "@ew-did-registry/keys": "0.8.1-alpha.996.0",
    "@ew-did-registry/revocation": "0.8.1-alpha.996.0",
    "base64-arraybuffer": "^1.0.2",
    "bs58": "^5.0.0",
    "crypto-js": "^4.1.1",
    "dotenv": "^16.0.1",
    "ethers": "^5.7.1",
    "firebase-admin": "^11.0.1",
    "iam-client-lib": "^7.0.0",
    "jsonschema": "^1.4.1",
    "jwt-decode": "^3.1.2",
    "luxon": "^3.0.3",
    "mocha": "^10.0.0",
    "moment": "^2.29.4",
    "nock": "^13.2.9",
    "node-fetch": "^3.2.10",
